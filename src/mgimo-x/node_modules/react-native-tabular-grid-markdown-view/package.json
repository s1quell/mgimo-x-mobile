  "author": {
    "name": "Benjamin Dobell",
    "email": "contact@glassechidna.com.au"
  },
  "contributors": [
    {
      "name": "Gethin Webster",
      "email": "gethin.webster@agilityworks.co.uk"
    },
    {
      "name": "Benjamin Dobell",
      "email": "contact@glassechidna.com.au"
    }
  ],
  "files": [
    "src"
  ],
  "main": "src/index.js",
  "keywords": [
    "react-native"
  ],
  "dependencies": {
    "prop-types": "^15.5.10"
  },
  "devDependencies": {
    "@taw/condition-vsts": "^1.0.5",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^15.0.0",
    "babel-preset-react-native": "^1.9.0",
    "cross-env": "^3.1.3",
    "eslint": "^3.1.1",
    "eslint-config-xo-space": "^0.15.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-react": "^6.3.0",
    "jasmine-reporters": "^2.2.0",
    "jest": "^15.1.1",
    "semantic-release": "^6.3.2"
  },
  "peerDependencies": {
    "react": ">=15.4.2",
    "react-native": ">=0.41.0"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "lintXML": "eslint . -f junit -o output/LINT-eslint.xml",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "lcov",
      "cobertura",
      "text"
    ],
    "setupTestFrameworkScriptFile": "jestSupport/setupJasmine.js",
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/example/"
    ],
    "automock": true,
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ]
  },
  "release": {
    "branch": "master",
    "verifyConditions": "@taw/condition-vsts"
  },
  "publishConfig": {},
  "eslintConfig": {
    "extends": "xo-space",
    "env": {
      "mocha": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Benjamin-Dobell/react-native-tabular-grid-markdown-view.git"
